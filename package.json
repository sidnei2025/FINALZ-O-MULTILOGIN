{
  "name": "sidnei-ferramentas",
  "productName": "Sidnei - Ferramentas Ilimitadas",
  "version": "1.0.0",
  "private": true,
  "main": "./electron/main.js",
  "homepage": "./",
  "description": "Sistema de multilogin avan√ßado estilo Netflix",
  "author": "Sidnei Inc",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.344.0",
    "proxy-chain": "^2.7.1",
    "puppeteer-core": "^22.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^32.0.0",
    "electron-builder": "^24.13.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "vite --port 5173",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:5173 && electron .\"",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win --x64",
    "dist:mac": "npm run build && electron-builder --mac --x64",
    "dist:mac-arm": "npm run build && electron-builder --mac --arm64",
    "dist:linux": "npm run build && electron-builder --linux --x64"
  },
  "build": {
    "appId": "com.sidnei.ferramentas",
    "productName": "Sidnei - Ferramentas Ilimitadas",
    "asar": true,
    "asarUnpack": [
      "**/*.dll",
      "**/*.node",
      "**/*.dylib"
    ],
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "extraFiles": [
      "browser/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "electron/icon.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "electron/icon.icns",
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "electron/icon.png",
      "category": "Utility"
    }
  }
}